{
  "name": "ecs-project",
  "version": "0.1.0",
  "bin": {
    "ecs-project": "bin/ecs-project.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "deploy": "./deploy.sh",
    "cleanup": "./cleanup.sh",
    "access": "./access.sh",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "lint": "echo 'Linting not configured yet'",
